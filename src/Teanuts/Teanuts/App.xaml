<?xml version="1.0" encoding="utf-8"?>
<Application
    x:Class="Teanuts.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:convert="using:Teanuts.Converter"
    xmlns:local="using:Teanuts">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <XamlControlsResources xmlns="using:Microsoft.UI.Xaml.Controls" />
                <ResourceDictionary Source="ms-appx:///Style/FontStyle.xaml" />
                <ResourceDictionary Source="ms-appx:///Style/Color.xaml"/>
                <ResourceDictionary Source="ms-appx:///Style/ApplicationUsageItem.xaml"/>
            </ResourceDictionary.MergedDictionaries>
            <convert:TimeSpanToHourMinConverter x:Key="TimeSpanToHourMinConverter"/>
            <convert:TimeSpanToRatioTransformConverter x:Key="TimeSpanToRatioTransformConverter"/>
        </ResourceDictionary>
    </Application.Resources>
</Application>
